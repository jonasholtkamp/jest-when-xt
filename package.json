{
  "name": "jest-when-xt",
  "version": "0.2.0",
  "description": "An extension lib for jest",
  "license": "MIT",
  "main": "src/when.js",
  "scripts": {
    "test": "jest",
    "standard": "standard --env jest",
    "stryker": "stryker run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonasholtkamp/jest-when-xt/"
  },
  "contributors": [
    "Jonas Holtkamp <jonas.holtkamp@senacor.com>"
  ],
  "dependencies": {
    "bunyan": "^1.8.12",
    "expect": "^27.0.6"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^5.2.2",
    "@stryker-mutator/jest-runner": "^5.2.2",
    "jest": "^27.0.6",
    "pre-commit": "^1.2.2",
    "standard": "^16.0.3"
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "build/reports/coverage/",
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "resetModules": true
  },
  "standard": {
    "env": {
      "browser": false,
      "node": true,
      "jest": true,
      "es2017": true
    }
  },
  "pre-commit": [
    "standard",
    "test"
  ]
}
