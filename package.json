{
  "name": "jest-when-xt",
  "version": "0.1.6",
  "description": "An extension lib for jest",
  "license": "MIT",
  "main": "src/when.js",
  "scripts": {
    "test": "jest",
    "standard": "standard --env jest",
    "stryker": "stryker run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonasholtkamp/jest-when-xt/"
  },
  "contributors": [
    "Jonas Holtkamp <jonas.holtkamp@senacor.com>"
  ],
  "dependencies": {
    "bunyan": "^1.8.14",
    "expect": "^24.1.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.3.1",
    "@stryker-mutator/javascript-mutator": "^3.3.1",
    "@stryker-mutator/jest-runner": "^3.3.1",
    "jest": "^24.1.0",
    "pre-commit": "^1.2.2",
    "standard": "^14.3.4"
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "build/reports/coverage/",
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "resetModules": true
  },
  "pre-commit": [
    "standard",
    "test"
  ]
}
