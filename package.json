{
  "name": "@thatisnomoon/jest-when-xt",
  "version": "0.0.3",
  "description": "An extension lib for jest",
  "main": "src/when.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint.fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.thatisnomoon.io/timeline-troopers/jest-when-xt/"
  },
  "contributors": [
    "Jonas Holtkamp <jonas.holtkamp@senacor.com>"
  ],
  "dependencies": {
    "bunyan": "^1.8.12",
    "expect": "^22.4.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.1.1",
    "pre-commit": "^1.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "build/reports/coverage/",
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
